{
  "name": "reactnativestrucure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:staging": "react-native run-android --variant=stagingdebug --appIdSuffix 'stage'",
    "android:staging-release": "react-native run-android --variant=stagingrelease --appIdSuffix 'stage'",
    "android:dev": "react-native run-android --variant=developmentdebug --appIdSuffix 'dev'",
    "android:dev-release": "react-native run-android --variant=developmentrelease --appIdSuffix 'dev'",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios:production": "react-native run-ios --scheme 'ReactNativeStrucure'",
    "ios:development": "react-native run-ios --scheme 'ReactNativeStrucureDev'",
    "ios:staging": "react-native run-ios --scheme 'ReactNativeStrucureStage'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "axios": "^0.21.4",
    "react": "17.0.2",
    "react-native": "^0.65.1",
    "react-native-appearance": "^0.3.4",
    "react-native-bootsplash": "^3.2.7",
    "react-native-config": "^1.4.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-htmlview": "^0.16.0",
    "react-native-media-controls": "^2.3.0",
    "react-native-orientation-locker": "^1.3.1",
    "react-native-paper": "^4.9.2",
    "react-native-pell-rich-editor": "^1.8.6",
    "react-native-reanimated": "^2.2.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-simple-toast": "^1.1.3",
    "react-native-slider": "^0.11.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-video": "^5.1.1",
    "react-native-video-controls": "^2.8.1",
    "react-native-webview": "^11.13.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "jest": "27.2.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "2.3.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
